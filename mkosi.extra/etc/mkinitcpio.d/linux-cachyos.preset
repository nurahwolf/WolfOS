# Customised mkinitcpio preset file for the 'linux-cachyos' package, using UKIs

#ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-cachyos"

PRESETS=('default')

#default_config="/etc/mkinitcpio.conf"
#default_image="/boot/moshi.img"
default_uki="/efi/EFI/Linux/moshi.efi"
#default_options="--splash /usr/share/systemd/bootctl/splash-arch.bmp"

#fallback_config="/etc/mkinitcpio.conf"
#fallback_image="/boot/moshi-fallback.img"
fallback_uki="/efi/EFI/Linux/moshi-fallback.efi"
#fallback_options="-S autodetect"
